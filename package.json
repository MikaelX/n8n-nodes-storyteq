{
	"name": "n8n-nodes-storyteq",
	"version": "1.0.6",
	"description": "n8n node for Storyteq API",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"n8n-nodes",
		"storyteq",
		"creative-automation",
		"cmp",
		"media-creation"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:MikaelX/n8n-nodes-storyteq.git"
	},
	"main": "index.js",
	"scripts": {
		"dev": "n8n-node dev",
		"build": "tsc --project tsconfig.build.cjs.json && node scripts/fix-node-exports.js",
		"build:watch": "n8n-node build --watch",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"typecheck": "tsc --noEmit",
		"pre-publish": "scripts/pre-publish-check.sh",
		"prepublishOnly": "npm run pre-publish"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"nodes": [
			"dist/nodes/Storyteq/Storyteq.node.js"
		],
		"credentials": [
			"dist/credentials/Storyteq.credentials.js"
		]
	},
	"dependencies": {
		"form-data": "4.0.5",
		"n8n-workflow": "^1.120.6"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@n8n/node-cli": "^0.17.0",
		"@n8n/typescript-config": "^1.2.0",
		"@types/node": "^25.0.9",
		"eslint": "^9.39.2",
		"globals": "^17.0.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1"
	},
	"peerDependencies": {
		"n8n-workflow": "^1.120.0"
	}
}
